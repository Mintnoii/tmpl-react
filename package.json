{
  "name": "tmpl-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "format": "prettier --write src/**/*.ts src/**/*.tsx",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "ahooks": "^3.6.2",
    "antd": "^4.22.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "windicss": "^3.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.10.3",
    "vite": "^3.0.0",
    "vite-plugin-imp": "^2.2.0",
    "vite-plugin-windicss": "^1.8.7"
  },
  "lint-staged": {
    "*.{ts,js,tsx}": [
      "eslint",
      "yarn lint",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "description": "## 技术栈",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mintnoii/tmpl-react.git"
  },
  "author": "Mintnoii",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mintnoii/tmpl-react/issues"
  },
  "homepage": "https://github.com/Mintnoii/tmpl-react#readme"
}
